{
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"]
  },
  "plugins": [
    "react"
  ]
}
